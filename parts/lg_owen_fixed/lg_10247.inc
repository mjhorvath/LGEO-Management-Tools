    LG_BEAM_WIDTH/2-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    translate <0, -0.08-LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_BRICK_WIDTH/2>
  }
  torus {
    LG_BEAM_WIDTH/2-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    translate <0, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_BRICK_WIDTH/2>
  }
  cylinder {
    <-LG_BEAM_WIDTH/2+LG_CORNER_SPACE, -0.08-LG_CORNER_SPACE, LG_PLATE_INNER_HEIGHT>,
    <-LG_BEAM_WIDTH/2+LG_CORNER_SPACE, -0.08-LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_BRICK_WIDTH/2>,
    LG_CORNER_SPACE
  }
  cylinder {
    <LG_BEAM_WIDTH/2-LG_CORNER_SPACE, -0.08-LG_CORNER_SPACE, LG_PLATE_INNER_HEIGHT>,
    <LG_BEAM_WIDTH/2-LG_CORNER_SPACE, -0.08-LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_BRICK_WIDTH/2>,
    LG_CORNER_SPACE
  }
  cylinder {
    <-LG_BEAM_WIDTH/2+LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_CORNER_SPACE>,
    <-LG_BEAM_WIDTH/2+LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_BRICK_WIDTH/2>,
    LG_CORNER_SPACE
  }
  cylinder {
    <LG_BEAM_WIDTH/2-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_CORNER_SPACE>,
    <LG_BEAM_WIDTH/2-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_BRICK_WIDTH/2>,
    LG_CORNER_SPACE
  }
  difference {
    merge {
      box {
        <-LG_BEAM_WIDTH/2, -0.08-LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_BRICK_WIDTH/2>,
        <LG_BEAM_WIDTH/2, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_PLATE_INNER_HEIGHT+LG_CORNER_SPACE>
      }
      box {
        <-LG_BEAM_WIDTH/2+LG_CORNER_SPACE, -0.08, LG_PLATE_HEIGHT-LG_BRICK_WIDTH/2>,
        <LG_BEAM_WIDTH/2-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH, LG_PLATE_INNER_HEIGHT>
      }
      cylinder {
        <0, -0.08, LG_PLATE_HEIGHT-LG_BRICK_WIDTH/2>,
        <0, LENGTH/2*-LG_BRICK_WIDTH, LG_PLATE_HEIGHT-LG_BRICK_WIDTH/2>,
        LG_BEAM_WIDTH/2-LG_CORNER_SPACE
      }
      cylinder {
        <0, -0.08-LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_BRICK_WIDTH/2>,
        <0, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_BRICK_WIDTH/2>,
        LG_BEAM_WIDTH/2
      }
    }
    cylinder {
      <0, 0, LG_PLATE_HEIGHT-LG_BRICK_WIDTH/2>,
      <0, -0.08-LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_BRICK_WIDTH/2>,
      LG_KNOB_RADIUS+LG_CORNER_SPACE
    }
    cylinder {
      <0, 0, LG_PLATE_HEIGHT-LG_BRICK_WIDTH/2>,
      <0, LENGTH/2*-LG_BRICK_WIDTH, LG_PLATE_HEIGHT-LG_BRICK_WIDTH/2>,
      LG_KNOB_RADIUS
    }
    object {
      lg_tech_hole
      rotate <0, 0, 90>
      translate <0, LENGTH/2*-LG_BRICK_WIDTH, LG_PLATE_HEIGHT-LG_BRICK_WIDTH/2>
    }
  }
  // UNDERSIDE STUD
  torus {
    LG_KNOB_INNER_RADIUS-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <90, 0, 0>
    translate <0, 0.5*LG_BRICK_WIDTH, LG_CORNER_SPACE>
  }
  cylinder {
    <0, 0.5*LG_BRICK_WIDTH, 0>,
    <0, 0.5*LG_BRICK_WIDTH, LG_PLATE_INNER_HEIGHT-LG_CORNER_SPACE>,
    LG_KNOB_INNER_RADIUS-LG_CORNER_SPACE
  }
  cylinder {
    <0, 0.5*LG_BRICK_WIDTH, LG_CORNER_SPACE>,
    <0, 0.5*LG_BRICK_WIDTH, LG_PLATE_INNER_HEIGHT>,
    LG_KNOB_INNER_RADIUS
  }
  // STUDS
  object {
    lg_tech_knob_logo_group_merge (2, 2)
    translate <0, 0, LG_PLATE_HEIGHT>
  }
  translate <0, 0, -LG_PLATE_HEIGHT>
}

#end

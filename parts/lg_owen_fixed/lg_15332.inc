    cylinder {
      <0, 0, LG_PLATE_HEIGHT+LG_KNOB_HEIGHT>,
      <0, 0, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT>,
      LG_KNOB_INNER_RADIUS

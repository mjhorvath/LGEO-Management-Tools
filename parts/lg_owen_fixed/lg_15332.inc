  cylinder {
    <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>,

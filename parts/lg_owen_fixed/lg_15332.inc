    0.08-LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*-LG_BRICK_WIDTH+0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>,

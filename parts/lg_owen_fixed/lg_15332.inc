  }
  cylinder {
    <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*-LG_BRICK_WIDTH+0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT>,
    <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*-LG_BRICK_WIDTH+0.08, HEIGHT*LG_BRICK_HEIGHT>,
    0.08-LG_CORNER_SPACE
  }
  #declare KNOB_X = 0;
  #while (KNOB_X < LENGTH)
  object {
    lg_tech_knob_logo
    translate <0, (0.5+KNOB_X)*LG_BRICK_WIDTH, 0>
    translate <0, LENGTH/2*-LG_BRICK_WIDTH, HEIGHT*LG_BRICK_HEIGHT>
    }
  #declare KNOB_X = KNOB_X + 1;
  #end
  translate <0, 0, HEIGHT*-LG_BRICK_HEIGHT>
}


  }
  cylinder {
    <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_CORNER_SPACE>,
    <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  sphere {
    <WIDTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  sphere {
    <WIDTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  sphere {
    <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  sphere {
    <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_CORNER_SPACE>,
    <WIDTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_CORNER_SPACE>,
    <WIDTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_CORNER_SPACE>,
    <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_CORNER_SPACE>,
    <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    <WIDTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  difference {
    merge {
      box {
        <WIDTH/2*-LG_BRICK_WIDTH, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_CORNER_SPACE>,
        <WIDTH/2*LG_BRICK_WIDTH, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>
      }
      box {
        <WIDTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH, LG_CORNER_SPACE>,
        <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH, LG_PLATE_HEIGHT-LG_CORNER_SPACE>
      }
      box {
        <WIDTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, 0>,
        <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_PLATE_HEIGHT>
      }
    }
    box {
      <WIDTH/2*-LG_BRICK_WIDTH+LG_WALL_WIDTH, LENGTH/2*LG_BRICK_WIDTH-LG_WALL_WIDTH, -LG_E>,
      <WIDTH/2*LG_BRICK_WIDTH-LG_WALL_WIDTH, LENGTH/2*-LG_BRICK_WIDTH+LG_WALL_WIDTH, LG_PLATE_INNER_HEIGHT>
    }
    #declare KIS_X = 0;
    #while (KIS_X < LENGTH)
    object {
      lg_knob_inner_space
      translate <0, (0.5+KIS_X)*LG_BRICK_WIDTH, 0>
      translate <0, LENGTH/2*-LG_BRICK_WIDTH, LG_PLATE_INNER_HEIGHT>
    }
    #declare KIS_X = KIS_X + 1;
    #end
  }
  // UNDERSIDE COLUMNS
  #declare COL_X = 1;
  #while (COL_X < LENGTH)
  object {
    lg_plate_column_clear
    translate <0, COL_X*LG_BRICK_WIDTH, 0>
    translate <0, LENGTH/2*-LG_BRICK_WIDTH, 0>
  }
  #declare COL_X = COL_X + 1;
  #end
  // SPINDLES
  #declare SPINDLE_X = 0;
  #while (SPINDLE_X < LENGTH)
  merge {
    torus {
      LG_KNOB_RADIUS-LG_CORNER_SPACE,
      LG_CORNER_SPACE
      rotate <90, 0, 0>
      translate <0, 0, LG_PLATE_HEIGHT+LG_KNOB_HEIGHT-LG_CORNER_SPACE>
    }
    cylinder {
      <0, 0, LG_PLATE_HEIGHT>,
      <0, 0, LG_PLATE_HEIGHT+LG_KNOB_HEIGHT-LG_CORNER_SPACE>,
      LG_KNOB_RADIUS
    }
    cylinder {
      <0, 0, LG_PLATE_HEIGHT>,
      <0, 0, LG_PLATE_HEIGHT+LG_KNOB_HEIGHT>,
      LG_KNOB_RADIUS-LG_CORNER_SPACE
    }
    cylinder {
      <0, 0, LG_PLATE_HEIGHT+LG_KNOB_HEIGHT>,
      <0, 0, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT>,
      LG_KNOB_INNER_RADIUS
    }
    translate <0, (0.5+SPINDLE_X)*LG_BRICK_WIDTH, 0>
    translate <0, LENGTH/2*-LG_BRICK_WIDTH, 0>
  }
  #declare SPINDLE_X = SPINDLE_X + 1;
  #end
  // TOP
  torus {
    0.08-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <90, 0, 0>
    translate <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>
  }
  torus {
    0.08-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <90, 0, 0>
    translate <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*-LG_BRICK_WIDTH+0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>
  }
  torus {
    0.08-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <90, 0, 0>
    translate <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>
  }
  torus {
    0.08-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <90, 0, 0>
    translate <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*-LG_BRICK_WIDTH+0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>
  }
  cylinder {
    <WIDTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>,
    <WIDTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>,
    <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>,
    <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>,
    <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  torus {
    0.08-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <90, 0, 0>
    translate <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>
  }
  torus {
    0.08-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <90, 0, 0>
    translate <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*-LG_BRICK_WIDTH+0.08, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>
  }
  torus {
    0.08-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <90, 0, 0>
    translate <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>
  }
  torus {
    0.08-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <90, 0, 0>
    translate <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*-LG_BRICK_WIDTH+0.08, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>
  }
  cylinder {
    <WIDTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
    <WIDTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+0.08, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
    <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
    <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
    <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+0.08, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  difference {
    merge {
      box {
        <WIDTH/2*-LG_BRICK_WIDTH, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>,
        <WIDTH/2*LG_BRICK_WIDTH, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>
      }
      box {
        <WIDTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>,
        <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>
      }
      box {
        <WIDTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT>,
        <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, HEIGHT*LG_BRICK_HEIGHT>
      }
    }
    #declare MIR = 0;
    #while (MIR < 2)
    merge {
      box {
        <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT-LG_E>,
        <WIDTH/2*-LG_BRICK_WIDTH-LG_E, LENGTH/2*LG_BRICK_WIDTH+LG_E, HEIGHT*LG_BRICK_HEIGHT+LG_E>
      }
      box {
        <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT-LG_E>,
        <WIDTH/2*LG_BRICK_WIDTH+LG_E, LENGTH/2*LG_BRICK_WIDTH+LG_E, HEIGHT*LG_BRICK_HEIGHT+LG_E>
      }
      #if (MIR = 1)
      scale <0, -1, 0>
      #end
    }
    #declare MIR = MIR + 1;
    #end
  }
  cylinder {
    <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>,
    <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
    0.08
  }
  cylinder {
    <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT>,
    <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT>,
    0.08-LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*-LG_BRICK_WIDTH+0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>,
    <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*-LG_BRICK_WIDTH+0.08, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
    0.08
  }
  cylinder {
    <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*-LG_BRICK_WIDTH+0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT>,
    <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*-LG_BRICK_WIDTH+0.08, HEIGHT*LG_BRICK_HEIGHT>,
    0.08-LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>,
    <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
    0.08
  }
  cylinder {
    <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT>,
    <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT>,
    0.08-LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*-LG_BRICK_WIDTH+0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>,
    <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*-LG_BRICK_WIDTH+0.08, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
    0.08
  }
  cylinder {
    <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*-LG_BRICK_WIDTH+0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT>,
    <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*-LG_BRICK_WIDTH+0.08, HEIGHT*LG_BRICK_HEIGHT>,
    0.08-LG_CORNER_SPACE
  }
  #declare KNOB_X = 0;
  #while (KNOB_X < LENGTH)
  object {
    lg_tech_knob_logo_clear
    translate <0, (0.5+KNOB_X)*LG_BRICK_WIDTH, 0>
    translate <0, LENGTH/2*-LG_BRICK_WIDTH, HEIGHT*LG_BRICK_HEIGHT>
    }
  #declare KNOB_X = KNOB_X + 1;
  #end
  translate <0, 0, HEIGHT*-LG_BRICK_HEIGHT>
}

#end

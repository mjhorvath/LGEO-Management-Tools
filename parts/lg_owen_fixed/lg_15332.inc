  // UNDERSIDE COLUMNS
  #declare COL_X = 1;
  #while (COL_X < LENGTH)
  object {
    lg_plate_column
    translate <0, COL_X*LG_BRICK_WIDTH, 0>
    translate <0, LENGTH/2*-LG_BRICK_WIDTH, 0>
  }
  #declare COL_X = COL_X + 1;
  #end
  // SPINDLES
  #declare SPINDLE_X = 0;
  #while (SPINDLE_X < LENGTH)
  union {
    torus {
      LG_KNOB_RADIUS-LG_CORNER_SPACE,
      LG_CORNER_SPACE
      rotate <90, 0, 0>
      translate <0, 0, LG_PLATE_HEIGHT+LG_KNOB_HEIGHT-LG_CORNER_SPACE>
    }
    cylinder {
      <0, 0, LG_PLATE_HEIGHT>,
      <0, 0, LG_PLATE_HEIGHT+LG_KNOB_HEIGHT-LG_CORNER_SPACE>,
      LG_KNOB_RADIUS

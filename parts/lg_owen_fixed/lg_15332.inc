  }
  cylinder {
    <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>,
    <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>,
    <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>,
    <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  torus {
    0.08-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <90, 0, 0>
    translate <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>
  }
  torus {
    0.08-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <90, 0, 0>
    translate <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*-LG_BRICK_WIDTH+0.08, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>
  }
  torus {
    0.08-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <90, 0, 0>
    translate <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>
  }
  torus {
    0.08-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <90, 0, 0>
    translate <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*-LG_BRICK_WIDTH+0.08, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>
  }
  cylinder {
    <WIDTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>,

    }
    #declare KIS_X = 0;
    #while (KIS_X < LENGTH)
    object {
      lg_knob_inner_space
      translate <0, (0.5+KIS_X)*LG_BRICK_WIDTH, 0>
      translate <0, LENGTH/2*-LG_BRICK_WIDTH, LG_PLATE_INNER_HEIGHT>

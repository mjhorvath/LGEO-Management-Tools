      box {
        <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT-LG_E>,
        <WIDTH/2*LG_BRICK_WIDTH+LG_E, LENGTH/2*LG_BRICK_WIDTH+LG_E, HEIGHT*LG_BRICK_HEIGHT+LG_E>
      }
      #if (MIR = 1)
      scale <0, -1, 0>
      #end
    }
    #declare MIR = MIR + 1;
    #end
  }
  cylinder {
    <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>,
    <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_CORNER_SPACE>,

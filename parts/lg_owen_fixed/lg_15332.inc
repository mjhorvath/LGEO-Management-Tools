    translate <0, (0.5+SPINDLE_X)*LG_BRICK_WIDTH, 0>
    translate <0, LENGTH/2*-LG_BRICK_WIDTH, 0>
  }
  #declare SPINDLE_X = SPINDLE_X + 1;
  #end
  // TOP
  torus {
    0.08-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <90, 0, 0>
    translate <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>
  }
  torus {
    0.08-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <90, 0, 0>
    translate <WIDTH/2*-LG_BRICK_WIDTH+0.08, LENGTH/2*-LG_BRICK_WIDTH+0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>
  }
  torus {
    0.08-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <90, 0, 0>
    translate <WIDTH/2*LG_BRICK_WIDTH-0.08, LENGTH/2*LG_BRICK_WIDTH-0.08, HEIGHT*LG_BRICK_HEIGHT-LG_KNOB_HEIGHT+LG_CORNER_SPACE>
  }

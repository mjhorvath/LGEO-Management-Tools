    <0, LG_CORNER_SPACE, LG_CORNER_SPACE>,
    <WIDTH*LG_BRICK_WIDTH, LENGTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>
   }
   box {
    <LG_CORNER_SPACE, LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_CORNER_SPACE>,
    <WIDTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, (LENGTH-1)*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_BRICK_HEIGHT>
   }
   box {
    <LG_CORNER_SPACE, LG_BRICK_WIDTH-2*LG_CORNER_SPACE, LG_CORNER_SPACE>,
    <WIDTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, (LENGTH-1)*LG_BRICK_WIDTH, LG_BRICK_HEIGHT-LG_CORNER_SPACE>
   }
   box {
    <0, LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_CORNER_SPACE>,
    <WIDTH*LG_BRICK_WIDTH, (LENGTH-1)*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_BRICK_HEIGHT-LG_CORNER_SPACE>
   }
  }
  box {
   <LG_WALL_WIDTH, LG_BRICK_WIDTH+LG_WALL_WIDTH, -LG_E>,
   <WIDTH*LG_BRICK_WIDTH-LG_WALL_WIDTH, 3*LG_BRICK_WIDTH-LG_WALL_WIDTH, LG_BRICK_INNER_HEIGHT>
  }
  box {
   <LG_WALL_WIDTH, LG_WALL_WIDTH, -LG_E>,
   <WIDTH*LG_BRICK_WIDTH-LG_WALL_WIDTH, LG_BRICK_WIDTH-LG_WALL_WIDTH, LG_PLATE_INNER_HEIGHT>
  }
  box {
   <LG_WALL_WIDTH, (LENGTH-1)*LG_BRICK_WIDTH+LG_WALL_WIDTH, -LG_E>,
   <WIDTH*LG_BRICK_WIDTH-LG_WALL_WIDTH, LENGTH*LG_BRICK_WIDTH-LG_WALL_WIDTH, LG_PLATE_INNER_HEIGHT>
  }
  box {
   <LG_WALL_WIDTH, LG_BRICK_WIDTH-LG_CYLINDER_WALL_WIDTH/2, -LG_E>,
   <WIDTH*LG_BRICK_WIDTH-LG_WALL_WIDTH, LG_BRICK_WIDTH+LG_CYLINDER_WALL_WIDTH/2, LG_PLATE_INNER_HEIGHT>
  }
  box {
   <LG_WALL_WIDTH, 3*LG_BRICK_WIDTH-LG_CYLINDER_WALL_WIDTH/2, -LG_E>,
   <WIDTH*LG_BRICK_WIDTH-LG_WALL_WIDTH, 3*LG_BRICK_WIDTH+LG_CYLINDER_WALL_WIDTH/2, LG_PLATE_INNER_HEIGHT>
  }
 }

        <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH, LG_PLATE_HEIGHT-LG_CORNER_SPACE>
      }
      box {
        <WIDTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, 0>,
        <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_PLATE_HEIGHT>
      }
    }
    box {
      <-LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_BRICK_WIDTH+LG_CORNER_SPACE, -LG_CORNER_SPACE>,
      <LG_BRICK_WIDTH+LG_CORNER_SPACE, -LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_CORNER_SPACE>
    }
    box {
      <-LG_BRICK_WIDTH, LG_BRICK_WIDTH, 0>,
      <LG_BRICK_WIDTH, -LG_BRICK_WIDTH, LG_PLATE_HEIGHT>
    }
    box {
      <-LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
      <LG_BRICK_WIDTH+LG_CORNER_SPACE, -LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_PLATE_HEIGHT+LG_CORNER_SPACE>
    }
    difference {
      box {
        <WIDTH/2*-LG_BRICK_WIDTH+LG_WALL_WIDTH, LENGTH/2*LG_BRICK_WIDTH-LG_WALL_WIDTH, -LG_CORNER_SPACE>,
        <WIDTH/2*LG_BRICK_WIDTH-LG_WALL_WIDTH, LENGTH/2*-LG_BRICK_WIDTH+LG_WALL_WIDTH, LG_PLATE_INNER_HEIGHT>
      }
      box {
        <-LG_BRICK_WIDTH-LG_WALL_WIDTH, LG_BRICK_WIDTH+LG_WALL_WIDTH, -LG_CORNER_SPACE-LG_E>,
        <LG_BRICK_WIDTH+LG_WALL_WIDTH, -LG_BRICK_WIDTH-LG_WALL_WIDTH, LG_PLATE_INNER_HEIGHT+LG_CORNER_SPACE>
      }
    }
    #declare ROT = 0;
    #while (ROT < 4)
    merge {
      object {
        lg_knob_inner_space
        translate <0.5*-LG_BRICK_WIDTH, 1.5*LG_BRICK_WIDTH, LG_PLATE_INNER_HEIGHT>
      }
      object {
        lg_knob_inner_space
        translate <1.5*-LG_BRICK_WIDTH, 1.5*LG_BRICK_WIDTH, LG_PLATE_INNER_HEIGHT>
      }
      object {
        lg_knob_inner_space
        translate <1.5*-LG_BRICK_WIDTH, 0.5*LG_BRICK_WIDTH, LG_PLATE_INNER_HEIGHT>
      }
      rotate <0, 0, 90*ROT>
    }
    #declare ROT = ROT + 1;
    #end
  }
  #declare ROT = 0;
  #while (ROT < 4)
  merge {
    object {
      lg_plate_column_clear
      translate <1*-LG_BRICK_WIDTH, 1.5*LG_BRICK_WIDTH, 0>
    }
    object {
      lg_plate_column_clear
      translate <1.5*-LG_BRICK_WIDTH, 1*LG_BRICK_WIDTH, 0>
    }
    object {
      lg_plate_column_clear
      translate <1.5*-LG_BRICK_WIDTH, 0, 0>
    }
    box {
      <LG_BRICK_WIDTH+3*LG_WALL_WIDTH, -0.04, 0.00001>,
      <LG_BRICK_WIDTH+3*LG_WALL_WIDTH+0.16, 0.04, LG_PLATE_INNER_HEIGHT>
      rotate <0, 0, 45>
    }
    object {
      lg_knob_clear
      rotate <0, 0, -90*ROT>
      translate <0.5*-LG_BRICK_WIDTH, 1.5*LG_BRICK_WIDTH, LG_PLATE_HEIGHT>
    }
    object {
      lg_knob_clear
      rotate <0, 0, -90*ROT>
      translate <1.5*-LG_BRICK_WIDTH, 1.5*LG_BRICK_WIDTH, LG_PLATE_HEIGHT>
    }
    object {
      lg_knob_clear
      rotate <0, 0, -90*ROT>
      translate <1.5*-LG_BRICK_WIDTH, 0.5*LG_BRICK_WIDTH, LG_PLATE_HEIGHT>
    }
    rotate <0, 0, 90*ROT>
  }
  #declare ROT = ROT + 1;
  #end
  translate <0, 0, -LG_PLATE_HEIGHT>
}

#end

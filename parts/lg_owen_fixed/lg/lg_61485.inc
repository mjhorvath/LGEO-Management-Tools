      0.30
    }
    cylinder {
      <0, 0, LG_PLATE_HEIGHT+LG_KNOB_HEIGHT-LG_CORNER_SPACE>,
      <0, 0, LG_PLATE_HEIGHT+LG_KNOB_HEIGHT+LG_E>,
      0.30+LG_CORNER_SPACE
    }
  }
  // OUTER TEETH
  difference {
    merge {
      cylinder {
        <0, 0, LG_PLATE_HEIGHT>,
        <0, 0, LG_PLATE_HEIGHT+LG_KNOB_HEIGHT>,
        1.43116
      }
      #declare ROT = 0;
      #while (ROT < 48)
      merge {
        torus {
          0.1024-LG_CORNER_SPACE,
          LG_CORNER_SPACE
          rotate <90, 0, 0>
          translate <0, 0, LG_PLATE_HEIGHT+LG_KNOB_HEIGHT-LG_CORNER_SPACE>
        }
        cylinder {
          <0, 0, LG_PLATE_HEIGHT-LG_E>
          <0, 0, LG_PLATE_HEIGHT+LG_KNOB_HEIGHT-LG_CORNER_SPACE>,
          0.1024
        }
        cylinder {
          <0, 0, LG_PLATE_HEIGHT>
          <0, 0, LG_PLATE_HEIGHT+LG_KNOB_HEIGHT>,
          0.1024-LG_CORNER_SPACE
        }
        scale <0.75, 1, 1>
        translate <1.43116, 0, 0>
        rotate <0, 0, 3.75>
        rotate <0, 0, 7.5*ROT>
      }
      #declare ROT = ROT + 1;
      #end
    }
    cylinder {
      <0, 0, LG_PLATE_HEIGHT-LG_E>,
      <0, 0, LG_PLATE_HEIGHT+LG_KNOB_HEIGHT>,
      1.32
    }
    cylinder {
      <0, 0, LG_PLATE_HEIGHT+LG_KNOB_HEIGHT-LG_CORNER_SPACE>,
      <0, 0, LG_PLATE_HEIGHT+LG_KNOB_HEIGHT+LG_E>,
      1.32+LG_CORNER_SPACE
    }
  }
  // UNDERSIDE STUDS
  #declare ROT = 0;
  #while (ROT < 4)
  merge {
    object {
      lg_plate_cylinder_clear
      translate <LG_BRICK_WIDTH, 0, 0>
    }
    object {
      lg_plate_cylinder_clear
      translate <LG_BRICK_WIDTH, LG_BRICK_WIDTH, 0>
    }
    rotate <0, 0, 90*ROT>
  }
  #declare ROT = ROT + 1;
  #end
  translate <0, 0, -LG_PLATE_HEIGHT>
}

#end

        <-LG_BRICK_WIDTH/2+LG_CORNER_SPACE, -LG_BRICK_WIDTH+LG_CORNER_SPACE, 0>,
        <LG_BRICK_WIDTH/2-LG_CORNER_SPACE, LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_PLATE_HEIGHT>
      }
      cylinder {
        <-LG_BRICK_WIDTH/2, 0, 0.5*LG_BRICK_WIDTH>,
        <LG_BRICK_WIDTH/2, 0, 0.5*LG_BRICK_WIDTH>,
        LG_BEAM_WIDTH/2-LG_CORNER_SPACE
      }
      cylinder {
        <-LG_BRICK_WIDTH/2+LG_CORNER_SPACE, 0, 0.5*LG_BRICK_WIDTH>,
        <LG_BRICK_WIDTH/2-LG_CORNER_SPACE, 0, 0.5*LG_BRICK_WIDTH>,
        LG_BEAM_WIDTH/2
      }
      cylinder {
        <-LG_BRICK_WIDTH/2, 0, 1.5*LG_BRICK_WIDTH>,
        <LG_BRICK_WIDTH/2, 0, 1.5*LG_BRICK_WIDTH>,
        LG_BEAM_WIDTH/2-LG_CORNER_SPACE
      }
      cylinder {
        <-LG_BRICK_WIDTH/2+LG_CORNER_SPACE, 0, 1.5*LG_BRICK_WIDTH>,
        <LG_BRICK_WIDTH/2-LG_CORNER_SPACE, 0, 1.5*LG_BRICK_WIDTH>,
        LG_BEAM_WIDTH/2
      }
      box {
        <-LG_WALL_WIDTH/2, -LG_BEAM_WIDTH/2+LG_E, LG_PLATE_HEIGHT>,
        <LG_WALL_WIDTH/2, LG_BEAM_WIDTH/2-LG_E, 1.5*LG_BRICK_WIDTH>
      }
      #declare MIR = 0;
      #while (MIR < 2)
      merge {
        cylinder {
          <-LG_BRICK_WIDTH/2+LG_CORNER_SPACE, -LG_BEAM_WIDTH/2+LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
          <-LG_BRICK_WIDTH/2+LG_CORNER_SPACE, -LG_BEAM_WIDTH/2+LG_CORNER_SPACE, 1.5*LG_BRICK_WIDTH>,
          LG_CORNER_SPACE
        }
        cylinder {
          <LG_BRICK_WIDTH/2-LG_CORNER_SPACE, -LG_BEAM_WIDTH/2+LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
          <LG_BRICK_WIDTH/2-LG_CORNER_SPACE, -LG_BEAM_WIDTH/2+LG_CORNER_SPACE, 1.5*LG_BRICK_WIDTH>,
          LG_CORNER_SPACE
        }
        cylinder {
          <-LG_BRICK_WIDTH/2+LG_CORNER_SPACE, -LG_BEAM_WIDTH/2+LG_WALL_WIDTH/1.5-LG_CORNER_SPACE, LG_PLATE_HEIGHT>,
          <-LG_BRICK_WIDTH/2+LG_CORNER_SPACE, -LG_BEAM_WIDTH/2+LG_WALL_WIDTH/1.5-LG_CORNER_SPACE, 1.5*LG_BRICK_WIDTH>,
          LG_CORNER_SPACE
        }
        cylinder {
          <LG_BRICK_WIDTH/2-LG_CORNER_SPACE, -LG_BEAM_WIDTH/2+LG_WALL_WIDTH/1.5-LG_CORNER_SPACE, LG_PLATE_HEIGHT>,
          <LG_BRICK_WIDTH/2-LG_CORNER_SPACE, -LG_BEAM_WIDTH/2+LG_WALL_WIDTH/1.5-LG_CORNER_SPACE, 1.5*LG_BRICK_WIDTH>,
          LG_CORNER_SPACE
        }
        box {
          <-LG_BRICK_WIDTH/2, -LG_BEAM_WIDTH/2+LG_WALL_WIDTH/1.5-LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
          <LG_BRICK_WIDTH/2, -LG_BEAM_WIDTH/2+LG_CORNER_SPACE, 1.5*LG_BRICK_WIDTH>
        }
        box {
          <-LG_BRICK_WIDTH/2+LG_CORNER_SPACE, -LG_BEAM_WIDTH/2+LG_WALL_WIDTH/1.5, LG_PLATE_HEIGHT>,
          <LG_BRICK_WIDTH/2-LG_CORNER_SPACE, -LG_BEAM_WIDTH/2, 1.5*LG_BRICK_WIDTH>
        }
        #if (MIR = 1)
        scale <1, -1, 1>
        #end
      }
      #declare MIR = MIR + 1;
      #end
    }
    box {
      <-LG_BRICK_WIDTH/2+LG_WALL_WIDTH, -LG_WALL_WIDTH, -LG_E>,
      <LG_BRICK_WIDTH/2-LG_WALL_WIDTH, -LG_BRICK_WIDTH+LG_WALL_WIDTH, LG_PLATE_INNER_HEIGHT>
    }
    box {
      <-LG_BRICK_WIDTH/2+LG_WALL_WIDTH, LG_WALL_WIDTH, -LG_E>,
      <LG_BRICK_WIDTH/2-LG_WALL_WIDTH, LG_BRICK_WIDTH-LG_WALL_WIDTH, LG_PLATE_INNER_HEIGHT>
    }
    cylinder {
      <-LG_BRICK_WIDTH/2, 0, 0.5*LG_BRICK_WIDTH>,
      <LG_BRICK_WIDTH/2, 0, 0.5*LG_BRICK_WIDTH>,
      LG_KNOB_RADIUS
    }
    cylinder {
      <-LG_BRICK_WIDTH/2-LG_E, 0, 0.5*LG_BRICK_WIDTH>,
      <-LG_BRICK_WIDTH/2+0.08, 0, 0.5*LG_BRICK_WIDTH>,
      0.3
    }
    cylinder {
      <LG_BRICK_WIDTH/2+LG_E, 0, 0.5*LG_BRICK_WIDTH>,
      <LG_BRICK_WIDTH/2-0.08, 0, 0.5*LG_BRICK_WIDTH>,
      0.3
    }
    cylinder {
      <-LG_BRICK_WIDTH/2-LG_E, 0, 0.5*LG_BRICK_WIDTH>,
      <-LG_BRICK_WIDTH/2+LG_CORNER_SPACE, 0, 0.5*LG_BRICK_WIDTH>,
      0.3+LG_CORNER_SPACE
    }
    cylinder {
      <LG_BRICK_WIDTH/2+LG_E, 0, 0.5*LG_BRICK_WIDTH>,
      <LG_BRICK_WIDTH/2-LG_CORNER_SPACE, 0, 0.5*LG_BRICK_WIDTH>,
      0.3+LG_CORNER_SPACE
    }
    cylinder {
      <-LG_BRICK_WIDTH/2, 0, 1.5*LG_BRICK_WIDTH>,
      <LG_BRICK_WIDTH/2, 0, 1.5*LG_BRICK_WIDTH>,
      LG_KNOB_RADIUS
    }
    cylinder {
      <-LG_BRICK_WIDTH/2-LG_E, 0, 1.5*LG_BRICK_WIDTH>,
      <-LG_BRICK_WIDTH/2+0.08, 0, 1.5*LG_BRICK_WIDTH>,
      0.3
    }
    cylinder {
      <LG_BRICK_WIDTH/2+LG_E, 0, 1.5*LG_BRICK_WIDTH>,
      <LG_BRICK_WIDTH/2-0.08, 0, 1.5*LG_BRICK_WIDTH>,
      0.3
    }
    cylinder {
      <-LG_BRICK_WIDTH/2-LG_E, 0, 1.5*LG_BRICK_WIDTH>,
      <-LG_BRICK_WIDTH/2+LG_CORNER_SPACE, 0, 1.5*LG_BRICK_WIDTH>,
      0.3+LG_CORNER_SPACE
    }
    cylinder {
      <LG_BRICK_WIDTH/2+LG_E, 0, 1.5*LG_BRICK_WIDTH>,
      <LG_BRICK_WIDTH/2-LG_CORNER_SPACE, 0, 1.5*LG_BRICK_WIDTH>,
      0.3+LG_CORNER_SPACE
    }
  }
  translate <0, 0, -LG_PLATE_HEIGHT>
}


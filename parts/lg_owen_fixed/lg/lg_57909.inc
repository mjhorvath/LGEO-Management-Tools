  // AXLE HOLE TEETH
  #declare ROT = 0;
  #while (ROT < 4)
  intersection {
    union {
      cylinder { 
        <LG_CROSSAXLE_WIDTH/2+0.08, LG_CROSSAXLE_WIDTH/2+0.08, LG_PLATE_HEIGHT+LG_CORNER_SPACE>,
        <LG_CROSSAXLE_WIDTH/2+0.08, LG_CROSSAXLE_WIDTH/2+0.08, LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
        0.08
      }
      cylinder { 
        <LG_CROSSAXLE_WIDTH/2+0.08, LG_CROSSAXLE_WIDTH/2+0.08, LG_PLATE_HEIGHT>,
        <LG_CROSSAXLE_WIDTH/2+0.08, LG_CROSSAXLE_WIDTH/2+0.08, LG_BRICK_HEIGHT>,
        0.08-LG_CORNER_SPACE
      }
      torus {
        0.08-LG_CORNER_SPACE,
        LG_CORNER_SPACE
        rotate <90, 0, 0>
        translate <LG_CROSSAXLE_WIDTH/2+0.08, LG_CROSSAXLE_WIDTH/2+0.08, LG_PLATE_HEIGHT+LG_CORNER_SPACE>

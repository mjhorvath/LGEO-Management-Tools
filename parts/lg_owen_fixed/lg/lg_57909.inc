      }
      cylinder { 
        <LG_CROSSAXLE_WIDTH/2+0.08, LG_CROSSAXLE_WIDTH/2+0.08, LG_PLATE_HEIGHT>,
        <LG_CROSSAXLE_WIDTH/2+0.08, LG_CROSSAXLE_WIDTH/2+0.08, LG_BRICK_HEIGHT>,
        0.08-LG_CORNER_SPACE
      }
      torus {
        0.08-LG_CORNER_SPACE,
        LG_CORNER_SPACE
        rotate <90, 0, 0>
        translate <LG_CROSSAXLE_WIDTH/2+0.08, LG_CROSSAXLE_WIDTH/2+0.08, LG_PLATE_HEIGHT+LG_CORNER_SPACE>
      }
      torus {
        0.08-LG_CORNER_SPACE,
        LG_CORNER_SPACE
        rotate <90, 0, 0>
        translate <LG_CROSSAXLE_WIDTH/2+0.08, LG_CROSSAXLE_WIDTH/2+0.08, LG_BRICK_HEIGHT-LG_CORNER_SPACE>
      }
      box {
        <LG_CROSSAXLE_WIDTH/2, LG_CROSSAXLE_WIDTH/2+0.08, LG_PLATE_HEIGHT+LG_CORNER_SPACE>,
        <LG_KNOB_RADIUS, LG_KNOB_RADIUS, LG_BRICK_HEIGHT-LG_CORNER_SPACE>
      }
      box {
        <LG_CROSSAXLE_WIDTH/2+LG_CORNER_SPACE, LG_CROSSAXLE_WIDTH/2+0.08, LG_PLATE_HEIGHT>,
        <LG_KNOB_RADIUS, LG_KNOB_RADIUS, LG_BRICK_HEIGHT>
      }
      cylinder {
        <LG_CROSSAXLE_WIDTH/2+LG_CORNER_SPACE, LG_CROSSAXLE_WIDTH/2+0.08, LG_PLATE_HEIGHT+LG_CORNER_SPACE>,
        <LG_CROSSAXLE_WIDTH/2+LG_CORNER_SPACE, LG_KNOB_RADIUS, LG_PLATE_HEIGHT+LG_CORNER_SPACE>,
        LG_CORNER_SPACE
      }
      cylinder {
        <LG_CROSSAXLE_WIDTH/2+LG_CORNER_SPACE, LG_CROSSAXLE_WIDTH/2+0.08, LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
        <LG_CROSSAXLE_WIDTH/2+LG_CORNER_SPACE, LG_KNOB_RADIUS, LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
        LG_CORNER_SPACE
      }
      box {
        <LG_CROSSAXLE_WIDTH/2+0.08, LG_CROSSAXLE_WIDTH/2, LG_PLATE_HEIGHT+LG_CORNER_SPACE>,
        <LG_KNOB_RADIUS, LG_KNOB_RADIUS, LG_BRICK_HEIGHT-LG_CORNER_SPACE>
      }
      box {
        <LG_CROSSAXLE_WIDTH/2+0.08, LG_CROSSAXLE_WIDTH/2+LG_CORNER_SPACE, LG_PLATE_HEIGHT>,
        <LG_KNOB_RADIUS, LG_KNOB_RADIUS, LG_BRICK_HEIGHT>
      }
      cylinder {
        <LG_CROSSAXLE_WIDTH/2+0.08, LG_CROSSAXLE_WIDTH/2+LG_CORNER_SPACE, LG_PLATE_HEIGHT+LG_CORNER_SPACE>,
        <LG_KNOB_RADIUS, LG_CROSSAXLE_WIDTH/2+LG_CORNER_SPACE, LG_PLATE_HEIGHT+LG_CORNER_SPACE>,
        LG_CORNER_SPACE
      }
      cylinder {
        <LG_CROSSAXLE_WIDTH/2+0.08, LG_CROSSAXLE_WIDTH/2+LG_CORNER_SPACE, LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
        <LG_KNOB_RADIUS, LG_CROSSAXLE_WIDTH/2+LG_CORNER_SPACE, LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
        LG_CORNER_SPACE
      }
    }
    cylinder {
      <0, 0, 0>,
      <0, 0, LG_BRICK_HEIGHT*2>,
      LG_CYLINDER_RADIUS-LG_CORNER_SPACE
    }
    rotate <0, 0, 90*ROT>
  }
  #declare ROT = ROT + 1;
  #end
  // BALL JOINT
  difference {
    cylinder {
      <0, LENGTH/2*-LG_BRICK_WIDTH+LG_E, LG_BRICK_HEIGHT-(LG_BRICK_WIDTH/2)>,
      <0, LENGTH/2*-LG_BRICK_WIDTH-LG_BRICK_WIDTH, LG_BRICK_HEIGHT-(LG_BRICK_WIDTH/2)>,
      LG_KNOB_RADIUS
    }
    #declare ROT = 0;
    #while (ROT < 4)
    box {
      <LG_CROSSAXLE_WIDTH/2, LENGTH/2*-LG_BRICK_WIDTH+LG_E, LG_CROSSAXLE_WIDTH/2>,
      <LG_KNOB_RADIUS, LENGTH/2*-LG_BRICK_WIDTH-LG_BRICK_WIDTH, LG_KNOB_RADIUS>
      rotate <0, 90*ROT, 0>
      translate <0, 0, LG_BRICK_HEIGHT-(LG_BRICK_WIDTH/2)>
    }
    #declare ROT = ROT + 1;
    #end
  }
  sphere {
    <0, LENGTH/2*-LG_BRICK_WIDTH-LG_BRICK_WIDTH, LG_BRICK_HEIGHT-(LG_BRICK_WIDTH/2)>,
    LG_BALL_JOINT
  }
  // STUDS
  object {
    lg_brick_cylinder_clear
  }
  #declare KNOB_W = 0;
  #while (KNOB_W < WIDTH)
  #declare KNOB_L = 0;
  #while (KNOB_L < LENGTH)
  object {
    lg_knob_clear
    translate <(0.5+KNOB_W)*LG_BRICK_WIDTH, (0.5+KNOB_L)*LG_BRICK_WIDTH, LG_BRICK_HEIGHT>
    translate <WIDTH/2*-LG_BRICK_WIDTH, LENGTH/2*-LG_BRICK_WIDTH, 0>
  }
  #declare KNOB_L = KNOB_L + 1;
  #end
  #declare KNOB_W = KNOB_W + 1;
  #end
  translate <0, 0, -LG_BRICK_HEIGHT>
}

#end

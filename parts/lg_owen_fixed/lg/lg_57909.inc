    cylinder {
      <0, 0, LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
      <0, 0, LG_BRICK_HEIGHT+LG_E>,
      LG_KNOB_RADIUS+LG_CORNER_SPACE
    }

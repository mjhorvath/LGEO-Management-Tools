      <WIDTH/2*LG_BRICK_WIDTH-LG_WALL_WIDTH, LENGTH/2*-LG_BRICK_WIDTH+LG_WALL_WIDTH, LG_BRICK_INNER_HEIGHT>
    }
    cylinder {
      <0, 0, LG_BRICK_INNER_HEIGHT-LG_E>,
      <0, 0, LG_BRICK_HEIGHT>,
      LG_KNOB_RADIUS

          <-LG_E, -LG_E, LG_BRICK_HEIGHT-LG_CORNER_SPACE-LG_BRICK_WIDTH/cos(pi*lg_angle2/180)*sin(lg_angle2*pi/180)>
        }
      }
      #declare KS_X = 0;
      #while (KS_X < LENGTH)
      object {
        lg_knob_inner_space
        translate <(KS_X+0.5)*LG_BRICK_WIDTH, 0.5*LG_BRICK_WIDTH, LG_PLATE_INNER_HEIGHT>
      }
      #declare KS_X = KS_X + 1;
      #end
    }
  }
  intersection {
    box {
      <LENGTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_CORNER_SPACE, 0>,
      <LG_CORNER_SPACE, -(LG_WALL_WIDTH-LG_CORNER_SPACE), -LG_BRICK_WIDTH/sin(pi*lg_angle1/180)>
      rotate <lg_angle1-1.15, 0, 0>
      translate <0, LG_BRICK_WIDTH, LG_BRICK_HEIGHT>
    }
    box {
      <LENGTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_BRICK_WIDTH+LG_CORNER_SPACE, 0>,
      <LG_CORNER_SPACE, WIDTH*LG_BRICK_WIDTH, LG_BRICK_HEIGHT-LG_CORNER_SPACE/4>
    }
  }
  #declare KNOB_X = 0;
  #while (KNOB_X < LENGTH)
  merge {
    object {
      lg_knob
      translate <(0.5+KNOB_X)*LG_BRICK_WIDTH, 0.5*LG_BRICK_WIDTH, LG_PLATE_HEIGHT>
    }
    object {
      lg_plate_column
      translate <(0.5+KNOB_X)*LG_BRICK_WIDTH, LG_BRICK_WIDTH, 0>
    }
  }
  #declare KNOB_X = KNOB_X + 1;
  #end
  translate <-LENGTH/2*LG_BRICK_WIDTH, -WIDTH/2*LG_BRICK_WIDTH, -LG_PLATE_HEIGHT>
  rotate <0, 0, -90>
}

#end

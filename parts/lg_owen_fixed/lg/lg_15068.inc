            <0, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, 2*LG_PLATE_HEIGHT>
          }
        }
        box {
          <WIDTH/2*-LG_BRICK_WIDTH+LG_WALL_WIDTH, LENGTH/2*LG_BRICK_WIDTH-LG_WALL_WIDTH, LG_PLATE_HEIGHT-LG_E>,
          <LG_E, LENGTH/2*-LG_BRICK_WIDTH+LG_WALL_WIDTH, 2*LG_PLATE_HEIGHT+LG_E>
        }
      }
      difference {
        merge {
          box {
            <0, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_CORNER_SPACE>,
            <WIDTH/2*LG_BRICK_WIDTH-LG_E, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, 2*LG_PLATE_HEIGHT-LG_CORNER_SPACE>
          }
          box {
            <LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH, LG_CORNER_SPACE>,
            <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH, 2*LG_PLATE_HEIGHT-LG_CORNER_SPACE>
          }
          box {
            <LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, 0>,
            <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, 2*LG_PLATE_HEIGHT>
          }
        }
        box {
          <LG_WALL_WIDTH, LENGTH/2*LG_BRICK_WIDTH-LG_WALL_WIDTH, -LG_E>,
          <WIDTH/2*LG_BRICK_WIDTH+LG_E, LENGTH/2*-LG_BRICK_WIDTH+LG_WALL_WIDTH, 2*LG_PLATE_HEIGHT+LG_E>
        }
      }
      object {
        lg_hollow_brick_column_clear
        translate <0.5*-LG_BRICK_WIDTH, 0, LG_PLATE_HEIGHT>
      }
      object {
        lg_hollow_brick_column_clear
        translate <0.5*LG_BRICK_WIDTH, 0, 0>
      }
    }
    cylinder {
      <WIDTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH+LG_E, 2*LG_PLATE_HEIGHT-LG_CURVE_RADIUS>,
      <WIDTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH-LG_E, 2*LG_PLATE_HEIGHT-LG_CURVE_RADIUS>,
      LG_CURVE_RADIUS-LG_CORNER_SPACE
      inverse
    }
  }
  difference {
    object {
      lg_curved_slope_top_merge (2, LG_CURVE_RADIUS, LG_CURVE_RADIUS-LG_WALL_WIDTH, LG_ANGLE)
      translate <WIDTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, 0, 2*LG_PLATE_HEIGHT-LG_CURVE_RADIUS>
    }
    box {
      <WIDTH/2*-LG_BRICK_WIDTH-LG_E, LENGTH/2*LG_BRICK_WIDTH+LG_E, LG_PLATE_HEIGHT>,
      <0.5*-LG_BRICK_WIDTH, LENGTH/2*-LG_BRICK_WIDTH-LG_E, LG_PLATE_HEIGHT+LG_KNOB_HEIGHT>
    }
    difference {
      box {
        <0.5*-LG_BRICK_WIDTH, 0.5*LG_BRICK_WIDTH, LG_PLATE_HEIGHT>,
        <LG_E, 0.5*-LG_BRICK_WIDTH, LG_PLATE_HEIGHT+LG_KNOB_HEIGHT>
      }
      box {
        <0.5*-LG_BRICK_WIDTH+LG_KNOB_RADIUS, 0.5*LG_BRICK_WIDTH, LG_PLATE_HEIGHT+0.12>,
        <0, 0.5*-LG_BRICK_WIDTH, LG_PLATE_HEIGHT+LG_KNOB_HEIGHT>
      }
    }
    cylinder {
      <0.5*-LG_BRICK_WIDTH, 0.5*LG_BRICK_WIDTH, LG_PLATE_HEIGHT>,
      <0.5*-LG_BRICK_WIDTH, 0.5*LG_BRICK_WIDTH, LG_PLATE_HEIGHT+LG_KNOB_HEIGHT>,
      LG_KNOB_RADIUS
    }
    cylinder {
      <0.5*-LG_BRICK_WIDTH, 0.5*-LG_BRICK_WIDTH, LG_PLATE_HEIGHT>,
      <0.5*-LG_BRICK_WIDTH, 0.5*-LG_BRICK_WIDTH, LG_PLATE_HEIGHT+LG_KNOB_HEIGHT>,
      LG_KNOB_RADIUS
    }
    box {
      <WIDTH/2*-LG_BRICK_WIDTH-LG_E, LENGTH/2*LG_BRICK_WIDTH+LG_E, -LG_E>,
      <WIDTH/2*LG_BRICK_WIDTH-LG_WALL_WIDTH/2, LENGTH/2*-LG_BRICK_WIDTH-LG_E, LG_KNOB_HEIGHT>
    }
  }
  // UNDERSIDE BOXES
  box {
    <0.5*-LG_BRICK_WIDTH+LG_KNOB_RADIUS, 0.5*LG_BRICK_WIDTH+0.06, LG_PLATE_HEIGHT>,
    <0, 0.5*LG_BRICK_WIDTH-0.06, LG_PLATE_HEIGHT+0.12>
  }
  box {
    <0.5*-LG_BRICK_WIDTH+LG_KNOB_RADIUS, 0.06, LG_PLATE_HEIGHT>,
    <0, -0.06, LG_PLATE_HEIGHT+0.12>
  }
  box {
    <0.5*-LG_BRICK_WIDTH+LG_KNOB_RADIUS, 0.5*-LG_BRICK_WIDTH-0.06, LG_PLATE_HEIGHT>,
    <0, 0.5*-LG_BRICK_WIDTH+0.06, LG_PLATE_HEIGHT+0.12>
  }
  // TIP FILLER
  box {
    <WIDTH/2*LG_BRICK_WIDTH-LG_WALL_WIDTH+LG_E, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_CORNER_SPACE>,
    <WIDTH/2*LG_BRICK_WIDTH, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_KNOB_HEIGHT-LG_CORNER_SPACE>
  }
  box {
    <WIDTH/2*LG_BRICK_WIDTH-LG_WALL_WIDTH, LENGTH/2*LG_BRICK_WIDTH-LG_WALL_WIDTH, 0>,
    <WIDTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_WALL_WIDTH, LG_KNOB_HEIGHT>
  }
}

#end

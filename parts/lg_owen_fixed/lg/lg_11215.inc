    <1*LG_BRICK_WIDTH-LG_KNOB_RADIUS, 0.5*LG_BRICK_WIDTH+LG_CYLINDER_WALL_WIDTH/1.5, LG_PLATE_INNER_HEIGHT>
  }
  box {
    <0.5*LG_BRICK_WIDTH, 0.5*-LG_BRICK_WIDTH-LG_CYLINDER_WALL_WIDTH/1.5, 0>,
    <1*LG_BRICK_WIDTH-LG_KNOB_RADIUS, 0.5*-LG_BRICK_WIDTH+LG_CYLINDER_WALL_WIDTH/1.5, LG_PLATE_INNER_HEIGHT>
  }
  // STUDS
  object {
    lg_brick_column_clear
  }
  object {
    lg_plate_cylinder_clear
    translate <1.5*-LG_BRICK_WIDTH, 0, (HEIGHT-1)*LG_PLATE_HEIGHT>
  }
  object {
    lg_plate_cylinder_clear
    translate <1.5*LG_BRICK_WIDTH, 0, 0>
  }
  #declare MIR = 0;
  #while (MIR < 2)
  merge {
    object {
      lg_knob_clear
      rotate <0, 0, -90>
      #if (MIR = 1) scale <1, -1, 1> #end
      translate <2*-LG_BRICK_WIDTH, 0.5*LG_BRICK_WIDTH, HEIGHT*LG_PLATE_HEIGHT>
    }
    object {
      lg_knob_clear
      rotate <0, 0, -90>
      #if (MIR = 1) scale <1, -1, 1> #end
      translate <1*-LG_BRICK_WIDTH, 0.5*LG_BRICK_WIDTH, HEIGHT*LG_PLATE_HEIGHT>
    }
    object {
      lg_knob_clear
      rotate <0, 0, -90>
      #if (MIR = 1) scale <1, -1, 1> #end
      translate <0, 0.5*LG_BRICK_WIDTH, HEIGHT*LG_PLATE_HEIGHT>
    }
    object {
      lg_knob_clear
      rotate <0, 0, -90>
      #if (MIR = 1) scale <1, -1, 1> #end
      translate <1*LG_BRICK_WIDTH, 0.5*LG_BRICK_WIDTH, LG_PLATE_HEIGHT>
    }
    object {
      lg_knob_clear
      rotate <0, 0, -90>
      #if (MIR = 1) scale <1, -1, 1> #end
      translate <2*LG_BRICK_WIDTH, 0.5*LG_BRICK_WIDTH, LG_PLATE_HEIGHT>
    }
    #if (MIR = 1)
    scale <1, -1, 1>
    #end
  }
  #declare MIR = MIR + 1;
  #end
  translate <0, 0, HEIGHT*-LG_PLATE_HEIGHT>
}

#end

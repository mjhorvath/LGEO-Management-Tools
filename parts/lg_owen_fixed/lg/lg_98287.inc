      <LENGTH/2*LG_BRICK_WIDTH-LG_WALL_WIDTH+LG_CORNER_SPACE, WIDTH*LG_BRICK_WIDTH-2*LG_WALL_WIDTH+LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
      <LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, WIDTH*LG_BRICK_WIDTH-LG_WALL_WIDTH+LG_CORNER_SPACE, HEIGHT*LG_BRICK_HEIGHT>
    }
    #if (MIR = 1)
    scale <-1, 1, 1>
    #end
  }
  #declare MIR = MIR + 1;
  #end
  #declare CYL_X = 1;
  #while (CYL_X < LENGTH)
  #declare CYL_Y = 1;
  #while (CYL_Y < WIDTH)
  object {
    lg_plate_cylinder_clear
    translate <CYL_X*LG_BRICK_WIDTH, CYL_Y*LG_BRICK_WIDTH, 0>
    translate <-LENGTH/2*LG_BRICK_WIDTH, 0, 0>
  }
  #declare CYL_Y = CYL_Y + 1;
  #end
  #declare CYL_X = CYL_X + 1;
  #end
  #declare CYL_X = 0.5;
  #while (CYL_X < LENGTH)
  #declare CYL_Y = 0.5;
  #while (CYL_Y < WIDTH-1)
  object {
    lg_knob_clear
    rotate <0, 0, -90>
    translate <CYL_X*LG_BRICK_WIDTH, CYL_Y*LG_BRICK_WIDTH, LG_PLATE_HEIGHT>
    translate <-LENGTH/2*LG_BRICK_WIDTH, 0, 0>
  }
  #declare CYL_Y = CYL_Y + 1;
  #end
  #declare CYL_X = CYL_X + 1;
  #end
  #declare CYL_X = 0.5;
  #while (CYL_X < LENGTH)
  object {
    lg_tech_knob_logo_clear
    rotate <270, 0, 0>
    rotate <0, 180, 0>
    translate <CYL_X*LG_BRICK_WIDTH, WIDTH*LG_BRICK_WIDTH, HEIGHT*LG_BRICK_HEIGHT-0.5*LG_BRICK_WIDTH>
    translate <-LENGTH/2*LG_BRICK_WIDTH, 0, 0>
  }
  #declare CYL_X = CYL_X + 1;
  #end
  object {
    lg_tech_knob_logo_clear
    rotate <270, 0, 0>
    rotate <0, 180, 0>
    translate <-LENGTH/2*LG_BRICK_WIDTH+(0.5*LG_BRICK_WIDTH), WIDTH*LG_BRICK_WIDTH, HEIGHT*LG_BRICK_HEIGHT-1.5*LG_BRICK_WIDTH>
  }
  object {
    lg_tech_knob_logo_clear
    rotate <270, 0, 0>
    rotate <0, 180, 0>
    translate <LENGTH/2*LG_BRICK_WIDTH-(0.5*LG_BRICK_WIDTH), WIDTH*LG_BRICK_WIDTH, HEIGHT*LG_BRICK_HEIGHT-1.5*LG_BRICK_WIDTH>
  }
  object {
    lg_tech_knob_logo_clear
    rotate <270, 0, 0>
    rotate <0, 180, 0>
    translate <-LENGTH/2*LG_BRICK_WIDTH+(0.5*LG_BRICK_WIDTH), WIDTH*LG_BRICK_WIDTH, HEIGHT*LG_BRICK_HEIGHT-2.5*LG_BRICK_WIDTH>
  }
  object {
    lg_tech_knob_logo_clear
    rotate <270, 0, 0>
    rotate <0, 180, 0>
    translate <LENGTH/2*LG_BRICK_WIDTH-(0.5*LG_BRICK_WIDTH), WIDTH*LG_BRICK_WIDTH, HEIGHT*LG_BRICK_HEIGHT-2.5*LG_BRICK_WIDTH>
  }
  rotate <0, 0, -90>
  translate <-WIDTH/2*LG_BRICK_WIDTH, 0, -LG_PLATE_HEIGHT>
}

#end

  }
  difference {
    merge {
      cylinder {
        <LG_BRICK_WIDTH/2, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,
        <LG_BRICK_WIDTH/2+0.32, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,

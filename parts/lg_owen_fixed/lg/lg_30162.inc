      LG_KNOB_INNER_RADIUS+LG_CORNER_SPACE
    }
    cylinder {
      <LG_BRICK_WIDTH/2+0.32-LG_CORNER_SPACE, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,
      <LG_BRICK_WIDTH/2+0.32+LG_E, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,
      LG_KNOB_INNER_RADIUS+LG_CORNER_SPACE
    }
    cylinder {
      <LG_BRICK_WIDTH/2+0.08, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,
      <LG_BRICK_WIDTH/2+0.32, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,
      LG_KNOB_INNER_RADIUS
    }
    cylinder {
      <LG_BRICK_WIDTH/2+0.08, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>,
      <LG_BRICK_WIDTH/2+0.32, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>,
      LG_KNOB_INNER_RADIUS
    }
    cylinder {
      <LG_BRICK_WIDTH/2-LG_E, 0, -Y2>,
      <LG_BRICK_WIDTH/2+LG_CORNER_SPACE, 0, -Y2>,
      LG_NEGATIVE_CYLINDER+LG_CORNER_SPACE
    }
    cylinder {
      <LG_BRICK_WIDTH/2, 0, -Y2>,
      <LG_BRICK_WIDTH/2+0.32, 0, -Y2>,
      LG_NEGATIVE_CYLINDER
    }
    cylinder {
      <LG_BRICK_WIDTH/2+0.32-LG_CORNER_SPACE, 0, -Y2>,
      <LG_BRICK_WIDTH/2+0.32+LG_E, 0, -Y2>,
      LG_NEGATIVE_CYLINDER+LG_CORNER_SPACE
    }
    cylinder {
      <LG_BRICK_WIDTH/2-LG_E, 0, Y2>,
      <LG_BRICK_WIDTH/2+LG_CORNER_SPACE, 0, Y2>,
      LG_NEGATIVE_CYLINDER+LG_CORNER_SPACE
    }
    cylinder {
      <LG_BRICK_WIDTH/2, 0, Y2>,
      <LG_BRICK_WIDTH/2+0.32, 0, Y2>,
      LG_NEGATIVE_CYLINDER
    }
    cylinder {
      <LG_BRICK_WIDTH/2+0.32-LG_CORNER_SPACE, 0, Y2>,
      <LG_BRICK_WIDTH/2+0.32+LG_E, 0, Y2>,
      LG_NEGATIVE_CYLINDER+LG_CORNER_SPACE
    }
  }

        LG_KNOB_RADIUS
      }
      box {
        <LG_BRICK_WIDTH/2, ZT, -YT>,
        <LG_BRICK_WIDTH/2+0.32, -ZT, YT>
      }
    }
    cylinder {
      <LG_BRICK_WIDTH/2+0.32-LG_CORNER_SPACE, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,
      <LG_BRICK_WIDTH/2+0.32+LG_E, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,
      LG_KNOB_INNER_RADIUS+LG_CORNER_SPACE
    }
    cylinder {
      <LG_BRICK_WIDTH/2+0.32-LG_CORNER_SPACE, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>,

  }
  torus {
    LG_KNOB_INNER_RADIUS-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <0, 0, 90>
    translate <-LG_BRICK_WIDTH/2+0.04-LG_CORNER_SPACE, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>
  }
  torus {
    LG_KNOB_INNER_RADIUS-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <0, 0, 90>
    translate <-LG_BRICK_WIDTH/2+0.08+LG_CORNER_SPACE, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>
  }
  torus {
    LG_KNOB_INNER_RADIUS-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <0, 0, 90>
    translate <-LG_BRICK_WIDTH/2+0.08+LG_CORNER_SPACE, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>
  }
  cylinder {
    <-LG_BRICK_WIDTH/2+0.08+LG_CORNER_SPACE, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, -LG_PLATE_HEIGHT/2+LG_CORNER_SPACE>,
    <-LG_BRICK_WIDTH/2+0.08+LG_CORNER_SPACE, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, -LG_PLATE_HEIGHT/2+LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <-LG_BRICK_WIDTH/2+0.08+LG_CORNER_SPACE, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, LG_PLATE_HEIGHT/2-LG_CORNER_SPACE>,
    <-LG_BRICK_WIDTH/2+0.08+LG_CORNER_SPACE, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, LG_PLATE_HEIGHT/2-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <-LG_BRICK_WIDTH/2+0.16-LG_CORNER_SPACE, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, -LG_PLATE_HEIGHT/2+LG_CORNER_SPACE>,
    <-LG_BRICK_WIDTH/2+0.16-LG_CORNER_SPACE, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, -LG_PLATE_HEIGHT/2+LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <-LG_BRICK_WIDTH/2+0.16-LG_CORNER_SPACE, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, LG_PLATE_HEIGHT/2-LG_CORNER_SPACE>,
    <-LG_BRICK_WIDTH/2+0.16-LG_CORNER_SPACE, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, LG_PLATE_HEIGHT/2-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <LG_BRICK_WIDTH/2-0.12+LG_CORNER_SPACE, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, -LG_PLATE_HEIGHT/2+LG_CORNER_SPACE>,
    <LG_BRICK_WIDTH/2-0.12+LG_CORNER_SPACE, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, -LG_PLATE_HEIGHT/2+LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <LG_BRICK_WIDTH/2-0.12+LG_CORNER_SPACE, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, LG_PLATE_HEIGHT/2-LG_CORNER_SPACE>,
    <LG_BRICK_WIDTH/2-0.12+LG_CORNER_SPACE, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, LG_PLATE_HEIGHT/2-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <LG_BRICK_WIDTH/2, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, -LG_PLATE_HEIGHT/2+LG_CORNER_SPACE>,
    <LG_BRICK_WIDTH/2, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, -LG_PLATE_HEIGHT/2+LG_CORNER_SPACE>,

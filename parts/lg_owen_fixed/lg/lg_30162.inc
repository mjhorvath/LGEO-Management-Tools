  cylinder {
    <LG_BRICK_WIDTH/2, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, LG_PLATE_HEIGHT/2-LG_CORNER_SPACE>,
    <LG_BRICK_WIDTH/2, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, LG_PLATE_HEIGHT/2-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  difference {
    merge {
      cylinder {
        <-LG_BRICK_WIDTH/2, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,
        <-LG_BRICK_WIDTH/2+0.04, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,
        LG_KNOB_INNER_RADIUS-LG_CORNER_SPACE
      }
      cylinder {
        <-LG_BRICK_WIDTH/2, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>,
        <-LG_BRICK_WIDTH/2+0.04, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>,
        LG_KNOB_INNER_RADIUS-LG_CORNER_SPACE
      }
      cylinder {
        <-LG_BRICK_WIDTH/2+LG_CORNER_SPACE, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,
        <-LG_BRICK_WIDTH/2+0.04-LG_CORNER_SPACE, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,
        LG_KNOB_INNER_RADIUS
      }
      cylinder {
        <-LG_BRICK_WIDTH/2+LG_CORNER_SPACE, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>,
        <-LG_BRICK_WIDTH/2+0.04-LG_CORNER_SPACE, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>,
        LG_KNOB_INNER_RADIUS
      }
      cylinder {
        <-LG_BRICK_WIDTH/2+0.04, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,
        <-LG_BRICK_WIDTH/2+0.08, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,
        LG_KNOB_INNER_RADIUS-0.04
      }
      cylinder {
        <-LG_BRICK_WIDTH/2+0.04, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>,
        <-LG_BRICK_WIDTH/2+0.08, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>,
        LG_KNOB_INNER_RADIUS-0.04
      }
      cylinder {
        <-LG_BRICK_WIDTH/2+0.08, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,
        <LG_BRICK_WIDTH/2, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,
        LG_KNOB_INNER_RADIUS-LG_CORNER_SPACE
      }
      cylinder {
        <-LG_BRICK_WIDTH/2+0.08, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>,
        <LG_BRICK_WIDTH/2, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>,
        LG_KNOB_INNER_RADIUS-LG_CORNER_SPACE
      }
      cylinder {
        <-LG_BRICK_WIDTH/2+0.08+LG_CORNER_SPACE, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,
        <LG_BRICK_WIDTH/2+LG_CORNER_SPACE, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,
        LG_KNOB_INNER_RADIUS
      }
      cylinder {
        <-LG_BRICK_WIDTH/2+0.08+LG_CORNER_SPACE, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>,
        <LG_BRICK_WIDTH/2+LG_CORNER_SPACE, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>,
        LG_KNOB_INNER_RADIUS
      }
      box {
        <-LG_BRICK_WIDTH/2+0.08, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, LG_PLATE_HEIGHT/2-LG_CORNER_SPACE>,
        <-LG_BRICK_WIDTH/2+0.16, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, -LG_PLATE_HEIGHT/2+LG_CORNER_SPACE>
      }
      box {
        <-LG_BRICK_WIDTH/2+0.08+LG_CORNER_SPACE, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, LG_PLATE_HEIGHT/2>,
        <-LG_BRICK_WIDTH/2+0.16-LG_CORNER_SPACE, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, -LG_PLATE_HEIGHT/2>
      }
      box {
        <LG_BRICK_WIDTH/2-0.12, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, LG_PLATE_HEIGHT/2-LG_CORNER_SPACE>,
        <LG_BRICK_WIDTH/2+LG_CORNER_SPACE, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, -LG_PLATE_HEIGHT/2+LG_CORNER_SPACE>
      }
      box {
        <LG_BRICK_WIDTH/2-0.12+LG_CORNER_SPACE, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, LG_PLATE_HEIGHT/2>,
        <LG_BRICK_WIDTH/2, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, -LG_PLATE_HEIGHT/2>
      }
    }
    cylinder {
      <-LG_BRICK_WIDTH/2-LG_E, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,
      <-LG_BRICK_WIDTH/2+LG_CORNER_SPACE, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,
      0.1+LG_CORNER_SPACE
    }
    cylinder {
      <-LG_BRICK_WIDTH/2-LG_E, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>,
      <-LG_BRICK_WIDTH/2+LG_CORNER_SPACE, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>,
      0.1+LG_CORNER_SPACE
    }
    cylinder {
      <-LG_BRICK_WIDTH/2, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,
      <-LG_BRICK_WIDTH/2+0.08-LG_CORNER_SPACE, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,
      0.1
    }
    cylinder {
      <-LG_BRICK_WIDTH/2, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>,
      <-LG_BRICK_WIDTH/2+0.08-LG_CORNER_SPACE, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>,
      0.1
    }
    torus {
      0.1-LG_CORNER_SPACE,
      LG_CORNER_SPACE
      rotate <0, 0, 90>
      translate <-LG_BRICK_WIDTH/2+0.08-LG_CORNER_SPACE, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>
    }
    torus {
      0.1-LG_CORNER_SPACE,
      LG_CORNER_SPACE
      rotate <0, 0, 90>
      translate <-LG_BRICK_WIDTH/2+0.08-LG_CORNER_SPACE, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>
    }
    cylinder {
      <-LG_BRICK_WIDTH/2, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,
      <-LG_BRICK_WIDTH/2+0.08, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>,
      0.1-LG_CORNER_SPACE
    }
    cylinder {
      <-LG_BRICK_WIDTH/2, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>,
      <-LG_BRICK_WIDTH/2+0.08, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>,
      0.1-LG_CORNER_SPACE
    }
    cylinder {
      <0, 0, -LG_PLATE_HEIGHT/2-LG_E>,
      <0, 0, -LG_PLATE_HEIGHT/2+LG_KNOB_HEIGHT>,
      LG_KNOB_RADIUS
    }
  }
  // BINCOLUAR LENSES
  torus {
    LG_KNOB_RADIUS-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <0, 0, 90>
    translate <LG_BRICK_WIDTH/2+LG_CORNER_SPACE, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>
  }
  torus {
    LG_KNOB_RADIUS-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <0, 0, 90>
    translate <LG_BRICK_WIDTH/2+LG_CORNER_SPACE, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>
  }
  torus {
    LG_KNOB_RADIUS-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <0, 0, 90>
    translate <LG_BRICK_WIDTH/2+0.32-LG_CORNER_SPACE, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>
  }
  torus {
    LG_KNOB_RADIUS-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <0, 0, 90>
    translate <LG_BRICK_WIDTH/2+0.32-LG_CORNER_SPACE, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>
  }
  torus {
    LG_KNOB_INNER_RADIUS+LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <0, 0, 90>
    translate <LG_BRICK_WIDTH/2+0.32-LG_CORNER_SPACE, LG_BRICK_WIDTH/2-LG_KNOB_INNER_RADIUS, 0>
  }
  torus {
    LG_KNOB_INNER_RADIUS+LG_CORNER_SPACE,
    LG_CORNER_SPACE
    rotate <0, 0, 90>
    translate <LG_BRICK_WIDTH/2+0.32-LG_CORNER_SPACE, -LG_BRICK_WIDTH/2+LG_KNOB_INNER_RADIUS, 0>
  }
  intersection {
    merge {
      torus {
        LG_NEGATIVE_CYLINDER+LG_CORNER_SPACE,
        LG_CORNER_SPACE
        rotate <0, 0, 90>
        translate <LG_BRICK_WIDTH/2+LG_CORNER_SPACE, 0, -Y2>
      }
      torus {
        LG_NEGATIVE_CYLINDER+LG_CORNER_SPACE,
        LG_CORNER_SPACE
        rotate <0, 0, 90>
        translate <LG_BRICK_WIDTH/2+0.32-LG_CORNER_SPACE, 0, -Y2>
      }
      torus {
        LG_NEGATIVE_CYLINDER+LG_CORNER_SPACE,
        LG_CORNER_SPACE
        rotate <0, 0, 90>
        translate <LG_BRICK_WIDTH/2+LG_CORNER_SPACE, 0, Y2>
      }
      torus {
        LG_NEGATIVE_CYLINDER+LG_CORNER_SPACE,
        LG_CORNER_SPACE
        rotate <0, 0, 90>
        translate <LG_BRICK_WIDTH/2+0.32-LG_CORNER_SPACE, 0, Y2>
      }
    }
    box {
      <LG_BRICK_WIDTH/2, ZT, -YT>,
      <LG_BRICK_WIDTH/2+0.32, -ZT, YT>

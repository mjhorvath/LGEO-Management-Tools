    <0.5*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_WALL_WIDTH/3-LG_CORNER_SPACE, -0.16>,
    LG_CORNER_SPACE
  }
  cylinder {
    <0.5*LG_BRICK_WIDTH-LG_CORNER_SPACE, -LG_WALL_WIDTH/3+LG_CORNER_SPACE, -2*LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    <0.5*LG_BRICK_WIDTH-LG_CORNER_SPACE, -LG_WALL_WIDTH/3+LG_CORNER_SPACE, -0.16>,
    LG_CORNER_SPACE
  }
  box {
    <-0.5*LG_BRICK_WIDTH+LG_WALL_WIDTH-LG_CORNER_SPACE, LG_WALL_WIDTH/3-LG_CORNER_SPACE, -2*LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    <0.5*LG_BRICK_WIDTH-0.16, -LG_WALL_WIDTH/3+LG_CORNER_SPACE, 0>
  }
  box {
    <-0.5*LG_BRICK_WIDTH+LG_WALL_WIDTH-LG_CORNER_SPACE, LG_WALL_WIDTH/3, -2*LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    <0.5*LG_BRICK_WIDTH-0.16, -LG_WALL_WIDTH/3, -LG_CORNER_SPACE>
  }
  box {
    <-0.5*LG_BRICK_WIDTH+LG_WALL_WIDTH-LG_CORNER_SPACE, LG_WALL_WIDTH/3-LG_CORNER_SPACE, -2*LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    <0.5*LG_BRICK_WIDTH, -LG_WALL_WIDTH/3+LG_CORNER_SPACE, -0.16>
  }
  box {
    <-0.5*LG_BRICK_WIDTH+LG_WALL_WIDTH-LG_CORNER_SPACE, LG_WALL_WIDTH/3, -2*LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    <0.5*LG_BRICK_WIDTH-LG_CORNER_SPACE, -LG_WALL_WIDTH/3, -0.16>
  }
  torus {
    0.16-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    translate <0.5*LG_BRICK_WIDTH-0.16, LG_WALL_WIDTH/3-LG_CORNER_SPACE, -0.16>
  }
  torus {
    0.16-LG_CORNER_SPACE,
    LG_CORNER_SPACE
    translate <0.5*LG_BRICK_WIDTH-0.16, -LG_WALL_WIDTH/3+LG_CORNER_SPACE, -0.16>
  }
  cylinder {
    <0.5*LG_BRICK_WIDTH-0.16, LG_WALL_WIDTH/3, -0.16>
    <0.5*LG_BRICK_WIDTH-0.16, -LG_WALL_WIDTH/3, -0.16>
    0.16-LG_CORNER_SPACE
  }
  cylinder {
    <0.5*LG_BRICK_WIDTH-0.16, LG_WALL_WIDTH/3-LG_CORNER_SPACE, -0.16>
    <0.5*LG_BRICK_WIDTH-0.16, -LG_WALL_WIDTH/3+LG_CORNER_SPACE, -0.16>
    0.16
  }
  object {
    lg_plate_column_clear
    translate <0, 0, -LG_BRICK_HEIGHT>
  }
}


  #declare CYL_X = 0.5;
  #while (CYL_X < LENGTH)
  #declare CYL_Z = 0.5;
  #while (CYL_Z < HEIGHT)
  object {
    lg_tech_knob_logo
    rotate <90, 0, 180>
    translate <CYL_X*LG_BRICK_WIDTH, LG_BRICK_WIDTH+LG_WALL_WIDTH, CYL_Z*LG_BRICK_WIDTH>
    translate <-LENGTH/2*LG_BRICK_WIDTH, 0, 0>
  }
  #declare CYL_Z = CYL_Z + 1;
  #end
  #declare CYL_X = CYL_X + 1;
  #end
  #declare CYL_X = 1;

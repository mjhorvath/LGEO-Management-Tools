    <LG_WALL_WIDTH-LG_CORNER_SPACE, WIDTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    <LG_WALL_WIDTH-LG_CORNER_SPACE, WIDTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_BRICK_HEIGHT-0.08>,
    LG_CORNER_SPACE
  }
  cylinder {
    <LENGTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_WALL_WIDTH-LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    <LENGTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_WALL_WIDTH-LG_CORNER_SPACE, LG_BRICK_HEIGHT-0.08>,
    LG_CORNER_SPACE
  }
  cylinder {
    <LG_WALL_WIDTH-LG_CORNER_SPACE, LG_WALL_WIDTH-LG_CORNER_SPACE, LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
    <LG_WALL_WIDTH-LG_CORNER_SPACE, WIDTH*LG_BRICK_WIDTH-0.08, LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <LG_WALL_WIDTH-LG_CORNER_SPACE, LG_WALL_WIDTH-LG_CORNER_SPACE, LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
    <LENGTH*LG_BRICK_WIDTH-0.08, LG_WALL_WIDTH-LG_CORNER_SPACE, LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <0, WIDTH*LG_BRICK_WIDTH-0.08, LG_BRICK_HEIGHT-0.08>,
    <LG_WALL_WIDTH, WIDTH*LG_BRICK_WIDTH-0.08, LG_BRICK_HEIGHT-0.08>,
    0.08-LG_CORNER_SPACE
  }
  cylinder {
    <LENGTH*LG_BRICK_WIDTH-0.08, 0, LG_BRICK_HEIGHT-0.08>,
    <LENGTH*LG_BRICK_WIDTH-0.08, LG_WALL_WIDTH, LG_BRICK_HEIGHT-0.08>,
    0.08-LG_CORNER_SPACE
  }
  cylinder {
    <LG_CORNER_SPACE, WIDTH*LG_BRICK_WIDTH-0.08, LG_BRICK_HEIGHT-0.08>,
    <LG_WALL_WIDTH-LG_CORNER_SPACE, WIDTH*LG_BRICK_WIDTH-0.08, LG_BRICK_HEIGHT-0.08>,
    0.08
  }
  cylinder {
    <LENGTH*LG_BRICK_WIDTH-0.08, LG_CORNER_SPACE, LG_BRICK_HEIGHT-0.08>,
    <LENGTH*LG_BRICK_WIDTH-0.08, LG_WALL_WIDTH-LG_CORNER_SPACE, LG_BRICK_HEIGHT-0.08>,
    0.08
  }
  difference {
    merge {
      box {
        <0, LG_CORNER_SPACE, LG_CORNER_SPACE>,
        <LG_BRICK_WIDTH, WIDTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>
      }
      box {
        <LG_CORNER_SPACE, 0, LG_CORNER_SPACE>,
        <LG_BRICK_WIDTH-LG_CORNER_SPACE, WIDTH*LG_BRICK_WIDTH, LG_PLATE_HEIGHT-LG_CORNER_SPACE>
      }
      box {
        <LG_CORNER_SPACE, LG_CORNER_SPACE, 0>,
        <LG_BRICK_WIDTH-LG_CORNER_SPACE, WIDTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_PLATE_HEIGHT>
      }
      box {
        <LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_CORNER_SPACE, LG_CORNER_SPACE>,
        <LENGTH*LG_BRICK_WIDTH, LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>
      }
      box {
        <LG_BRICK_WIDTH-LG_CORNER_SPACE, 0, LG_CORNER_SPACE>,
        <LENGTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_BRICK_WIDTH, LG_PLATE_HEIGHT-LG_CORNER_SPACE>
      }
      box {
        <LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_CORNER_SPACE, 0>,
        <LENGTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_PLATE_HEIGHT>
      }
    }
    box {
      <LG_WALL_WIDTH, LG_WALL_WIDTH, -LG_E>,
      <LG_BRICK_WIDTH-LG_WALL_WIDTH, WIDTH*LG_BRICK_WIDTH-LG_WALL_WIDTH, LG_PLATE_INNER_HEIGHT>
    }
    box {
      <LG_BRICK_WIDTH-LG_WALL_WIDTH-0.0001, LG_WALL_WIDTH, -LG_E>,
      <LENGTH*LG_BRICK_WIDTH-LG_WALL_WIDTH, LG_BRICK_WIDTH-LG_WALL_WIDTH, LG_PLATE_INNER_HEIGHT>
    }
  }
  box {
    <0, LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    <LG_WALL_WIDTH, WIDTH*LG_BRICK_WIDTH-0.08, LG_BRICK_HEIGHT-LG_CORNER_SPACE>
  }
  box {
    <LG_CORNER_SPACE, 0, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    <LG_WALL_WIDTH-LG_CORNER_SPACE, WIDTH*LG_BRICK_WIDTH-0.08, LG_BRICK_HEIGHT-LG_CORNER_SPACE>
  }
  box {
    <LG_CORNER_SPACE, LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    <LG_WALL_WIDTH-LG_CORNER_SPACE, WIDTH*LG_BRICK_WIDTH-0.08, LG_BRICK_HEIGHT>
  }
  box {
    <LG_CORNER_SPACE, WIDTH*LG_BRICK_WIDTH-0.08, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    <LG_WALL_WIDTH-LG_CORNER_SPACE, WIDTH*LG_BRICK_WIDTH, LG_BRICK_HEIGHT-0.08>
  }
  box {
    <0, WIDTH*LG_BRICK_WIDTH-0.08, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    <LG_WALL_WIDTH, WIDTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_BRICK_HEIGHT-0.08>
  }
  box {
    <LG_WALL_WIDTH-LG_CORNER_SPACE, 0, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    <LENGTH*LG_BRICK_WIDTH-0.08, LG_WALL_WIDTH, LG_BRICK_HEIGHT-LG_CORNER_SPACE>
  }
  box {
    <LG_WALL_WIDTH-LG_CORNER_SPACE, LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    <LENGTH*LG_BRICK_WIDTH-0.08, LG_WALL_WIDTH-LG_CORNER_SPACE, LG_BRICK_HEIGHT>
  }
  box {
    <LENGTH*LG_BRICK_WIDTH-0.08, LG_CORNER_SPACE, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    <LENGTH*LG_BRICK_WIDTH, LG_WALL_WIDTH-LG_CORNER_SPACE, LG_BRICK_HEIGHT-0.08>
  }
  box {
    <LENGTH*LG_BRICK_WIDTH-0.08, 0, LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
    <LENGTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_WALL_WIDTH, LG_BRICK_HEIGHT-0.08>
  }
  #declare CYL_Y = 1;
  #while (CYL_Y < WIDTH)
  object {
    lg_plate_column_clear
    translate <0.5*LG_BRICK_WIDTH, CYL_Y*LG_BRICK_WIDTH, 0>
  }
  #declare CYL_Y = CYL_Y + 1;
  #end
  #declare CYL_X = 1;
  #while (CYL_X < LENGTH)
  object {
    lg_plate_column_clear
    translate <CYL_X*LG_BRICK_WIDTH, 0.5*LG_BRICK_WIDTH, 0>
  }
  #declare CYL_X = CYL_X + 1;
  #end
  box {
    <0, LG_CYLINDER_WALL_WIDTH/1.5, 0>,
    <LG_KNOB_RADIUS, -LG_CYLINDER_WALL_WIDTH/1.5, LG_PLATE_INNER_HEIGHT>
    rotate <0, 0, -135>
    translate <LG_BRICK_WIDTH-LG_WALL_WIDTH/2, LG_BRICK_WIDTH-LG_WALL_WIDTH/2, 0>
  }
  rotate <0, 0, -90>
  translate <-0.5*LG_BRICK_WIDTH, 0.5*LG_BRICK_WIDTH, -LG_BRICK_HEIGHT>
}

#end

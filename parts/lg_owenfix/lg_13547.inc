      box {
        <3.5*LG_BRICK_WIDTH-LG_WALL_WIDTH/4, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_KNOB_HEIGHT>,
        <3.5*LG_BRICK_WIDTH+LG_WALL_WIDTH/4, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_BRICK_HEIGHT>
      }
    }
    cylinder {
      <2.5*LG_BRICK_WIDTH, 0, 2*LG_KNOB_HEIGHT>,

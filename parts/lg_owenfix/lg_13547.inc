  }
  cylinder {
    <2*LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
    <WIDTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_BRICK_HEIGHT-LG_KNOB_HEIGHT-LG_CORNER_SPACE>,
    <WIDTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_BRICK_HEIGHT-LG_KNOB_HEIGHT-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_BRICK_HEIGHT-LG_KNOB_HEIGHT-LG_CORNER_SPACE>,
    <WIDTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_BRICK_HEIGHT-LG_KNOB_HEIGHT-LG_CORNER_SPACE>,
    <WIDTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  cylinder {
    <WIDTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
    <WIDTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_BRICK_HEIGHT-LG_CORNER_SPACE>,
    LG_CORNER_SPACE
  }
  merge {
    torus {
      LG_CURVE_RADIUS-LG_CORNER_SPACE,
      LG_CORNER_SPACE
      translate <LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_CURVE_RADIUS>
    }
    torus {
      LG_CURVE_RADIUS-LG_CORNER_SPACE,
      LG_CORNER_SPACE
      translate <LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_CURVE_RADIUS>
    }
    clipped_by {
      plane {
        -x, -LG_BRICK_WIDTH-LG_CORNER_SPACE
      }
      plane {
        x, LG_BRICK_WIDTH-LG_CORNER_SPACE
        rotate <0, -LG_ANGLE, 0>
        translate <0, 0, LG_CURVE_RADIUS>
      }
    }
  }
  difference {
    merge {
      box {
        <0, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_PLATE_HEIGHT+LG_CORNER_SPACE>,
        <LG_BRICK_WIDTH+2*LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, 2*LG_PLATE_HEIGHT-LG_CORNER_SPACE>
      }
      box {
        <LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH, LG_PLATE_HEIGHT+LG_CORNER_SPACE>,
        <LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH, 2*LG_PLATE_HEIGHT-LG_CORNER_SPACE>
      }
      box {
        <LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_PLATE_HEIGHT>,
        <LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, 2*LG_PLATE_HEIGHT>
      }
    }
    box {
      <LG_WALL_WIDTH, LENGTH/2*LG_BRICK_WIDTH-LG_WALL_WIDTH, LG_PLATE_HEIGHT-LG_E>,
      <LG_BRICK_WIDTH+LG_WALL_WIDTH, LENGTH/2*-LG_BRICK_WIDTH+LG_WALL_WIDTH, LG_PLATE_HEIGHT+LG_PLATE_INNER_HEIGHT>
    }
    object {
      lg_knob_inner_space
      translate <0.5*LG_BRICK_WIDTH, 0, LG_PLATE_HEIGHT+LG_PLATE_INNER_HEIGHT>
    }
  }
  box {
    <0.5*LG_BRICK_WIDTH+LG_KNOB_RADIUS, LG_WALL_WIDTH/4, LG_PLATE_HEIGHT>,
    <1*LG_BRICK_WIDTH+LG_E, -LG_WALL_WIDTH/4, LG_PLATE_HEIGHT+LG_PLATE_INNER_HEIGHT>
  }
  difference {
    merge {
      box {
        <LG_BRICK_WIDTH, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, LG_CORNER_SPACE>,
        <WIDTH*LG_BRICK_WIDTH, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, 2*LG_PLATE_HEIGHT-LG_CORNER_SPACE>
      }
      box {
        <LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH, LG_CORNER_SPACE>,
        <WIDTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH, 2*LG_PLATE_HEIGHT-LG_CORNER_SPACE>
      }
      box {
        <LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, 0>,
        <WIDTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, 2*LG_PLATE_HEIGHT>
      }
      box {
        <2*LG_BRICK_WIDTH, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, 2*LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
        <WIDTH*LG_BRICK_WIDTH, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_BRICK_HEIGHT-LG_CORNER_SPACE>
      }
      box {
        <2*LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH, 2*LG_PLATE_HEIGHT-LG_CORNER_SPACE>,
        <WIDTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH, LG_BRICK_HEIGHT-LG_CORNER_SPACE>
      }
      box {
        <2*LG_BRICK_WIDTH+LG_CORNER_SPACE, LENGTH/2*LG_BRICK_WIDTH-LG_CORNER_SPACE, 2*LG_PLATE_HEIGHT>,
        <WIDTH*LG_BRICK_WIDTH-LG_CORNER_SPACE, LENGTH/2*-LG_BRICK_WIDTH+LG_CORNER_SPACE, LG_BRICK_HEIGHT>

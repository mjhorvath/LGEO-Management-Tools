        inverse
      }
      box {
        <-LG_E, LENGTH/2*LG_BRICK_WIDTH+LG_E, LG_BRICK_HEIGHT-LG_KNOB_HEIGHT-LG_CORNER_SPACE>,
        <WIDTH*LG_BRICK_WIDTH+LG_E, LENGTH/2*-LG_BRICK_WIDTH-LG_E, LG_BRICK_HEIGHT>
      }
    }
    box {
      <LG_BRICK_WIDTH+LG_WALL_WIDTH, LENGTH/2*LG_BRICK_WIDTH-LG_WALL_WIDTH, -LG_E>,
      <2*LG_BRICK_WIDTH, LENGTH/2*-LG_BRICK_WIDTH+LG_WALL_WIDTH, LG_PLATE_HEIGHT+LG_PLATE_INNER_HEIGHT>
    }
    box {
      <2*LG_BRICK_WIDTH+0.75*LG_WALL_WIDTH, LENGTH/2*LG_BRICK_WIDTH-0.75*LG_WALL_WIDTH, -LG_E>,
      <WIDTH*LG_BRICK_WIDTH-0.75*LG_WALL_WIDTH, LENGTH/2*-LG_BRICK_WIDTH+0.75*LG_WALL_WIDTH, LG_BRICK_HEIGHT+LG_E>
